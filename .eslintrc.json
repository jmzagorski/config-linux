{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "binaryLiterals": false,
      "blockBindings": true,
      "defaultParams": true,
      "forOf": true,
      "generators": true,
      "objectLiteralComputedProperties": false,
      "objectLiteralDuplicateProperties": false,
      "objectLiteralShorthandMethods": false,
      "objectLiteralShorthandProperties": false,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "templateStrings": true,
      "unicodeCodePointEscapes": false,
      "jsx": false,
      "modules": true
    }
  },

  "env": {
    "browser": false,
    "node": false,
    "amd": false,
    "mocha": false,
    "jasmine": false,
    "phantomjs": false,
    "jquery": false,
    "prototypejs": false,
    "shelljs": false
  },

  "globals": {
    "window": true,
    "Element": true,
    "document": true
  },

  "rules": {
    "comma-dangle": [ "error", "never" ],
    "no-cond-assign": [ "error", "always" ],
    "no-console": [ "warn" ],
    "no-constant-condition": [ "error", { "checkLoops": false } ],
    "no-control-regex": "error",
    "no-debugger": "error",
    "no-dupe-keys": "error",
    "no-empty": [ "error", { "allowEmptyCatch": true } ],
    "no-empty-character-class": "error",
    "no-ex-assign": "error",
    "no-extra-boolean-cast": "error",
    "no-extra-parens": "error",
    "no-extra-semi": "error",
    "no-func-assign": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 0,
    "no-obj-calls": 0,
    "no-regex-spaces": 0,
    "no-sparse-arrays": 0,
    "no-unreachable": 1,
    "use-isnan": 0,
    "valid-jsdoc": 1,
    "valid-typeof": 1,

    "block-scoped-var": 1,
    "complexity": 0,
    "consistent-return": 1,
    "curly": 0,
    "default-case": 0,
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-alert": 0,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-implied-eval": 0,
    "no-iterator": 0,
    "no-labels": "error",
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new": 1,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-octal": 0,
    "no-octal-escape": 0,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 1,
    "no-return-assign": 0,
    "no-script-url": 0,
    "no-self-compare": 1,
    "no-sequences": 0,
    "no-unused-expressions": 0,
    "no-void": 1,
    "no-warning-comments": 0,
    "no-with": 0,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": 1,
    "yoda": 0,

    "global-strict": 0,
    "no-extra-strict": 0,
    "strict": 1,

    "no-catch-shadow": 1,
    "no-delete-var": 1,
    "no-label-var": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-undef": 1,
    "no-undef-init": 1,
    "no-undefined": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,

    "brace-style": 1,
    "camelcase": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "consistent-this": 1,
    "eol-last": 1,
    "func-names": 1,
    "func-style": 1,
    "key-spacing": 1,
    "max-nested-callbacks": 0,
    "new-cap": 1,
    "new-parens": 1,
    "no-array-constructor": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": 1,
    "no-nested-ternary": 0,
    "no-new-object": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-wrap-func": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "padded-blocks": 0,
    "quote-props": [ "error", "as-needed" ],
    "quotes": [ 2, "double", { "allowTemplateLiterals": true } ],
    "semi-spacing": [ "error", {"before": false, "after": true} ],
    "semi": 0,
    "sort-vars": 0,
    "func-call-spacing": [ "error", "never" ],
    "keyword-spacing": [ "error" ],
    "space-before-blocks": 0,
    "space-in-brackets": 0,
    "space-in-parens": 0,
    "space-infix-ops": 1,
    "space-unary-ops": 0,
    "spaced-comment": [ "error", "always" ],
    "wrap-regex": 0,

    "no-var": 1,
    "generator-star": 0,

    "max-depth": 0,
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  }

}
